#include<stdio.h>
#include<string.h>
int main(void)
{
int len1,len2,i,j;
char a[100],b[100];
gets(a);
gets(b);
len1=strlen(a);
len2=strlen(b);
for(i=0;i<len1;i++)
{for(j=0;j<len2;j++)
{
if(a[i]>b[j])
printf("%s",a);
else if(a[i]<b[j])
printf("%s",b);
else if(a[i]==b[j])
printf("%s %s",a,b);
}
}
return 0;
}
